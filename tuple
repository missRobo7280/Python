#TUPLE : ordered, unchangeable, allow duplicates
tup1=(1,2,3,4,4)
print(type(tup1))
print(len(tup1))# length function
#creating one item TUPLE
tup2=("jaiswal",)#remember the comma
print(tup2)
#creating the tuple using the tuple constructor
tup4=tuple(("a",1,3,3,4,32,3,"swati"))
print(tup4)
print(*tup4)
